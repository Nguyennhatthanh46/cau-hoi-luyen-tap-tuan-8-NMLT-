#include <iostream>
#define MΑΧΝ 100
void NhapMang( <1> ,int &N){
std::cin >> N;
for (int i = 0; i < N; i++)
std::cin>>A[i];
}
void XuatMang( <2> ,const int &N) {
for(int i = N-1; i >= 0; i--)
std::cout<<A[i]<<" ";
}
